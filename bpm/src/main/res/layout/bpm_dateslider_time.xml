<?xml version="1.0" encoding="utf-8"?>
<com.example.spinel.myapplication.DateSlider.bpmSliderContainer
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/dateSliderContainer"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
    <com.example.spinel.myapplication.DateSlider.ScrollLayout
        style="@style/Scroller"
        app:labelerClass="com.example.spinel.myapplication.DateSlider.labeler.bpmMonthYearLabeler"
        app:labelerFormat="%tb %tY"
        app:childWidth="180dp"/>
    <com.example.spinel.myapplication.DateSlider.ScrollLayout
        style="@style/Scroller"
        app:labelerClass="com.example.spinel.myapplication.DateSlider.labeler.bpmDayDateLabeler"
        app:labelerFormat="%td %ta"
        app:childWidth="120dp"/>
    <com.example.spinel.myapplication.DateSlider.ScrollLayout
        style="@style/Scroller"
        app:labelerClass="com.example.spinel.myapplication.DateSlider.labeler.bpmTimeLabeler"
        app:labelerFormat="%tR"
        app:childWidth="80dp"/>
    <include layout="@layout/bpm_dialogbuttons"/>
</com.example.spinel.myapplication.DateSlider.bpmSliderContainer>
